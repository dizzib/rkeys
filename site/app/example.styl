// example.styl -- compiled and served as example.css at runtime

// use the built-in keys mixins...
@require 'mixin/keys'

// ...to set the fundamental look of each key
key-size(25mm)
key-color(blue, yellow)
label-color(white, blue)

body
  background-color  cyan

#Shift_L
  width   50mm

.section
  clear     both
  font-size 15pt
  padding   1em

#edit-cut
  background-color  red

#edit-copy
  background-color  green
