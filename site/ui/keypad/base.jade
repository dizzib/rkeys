// base template -- override any blocks to customise to your requirement
include mixins
doctype
html
  head
    block meta

    block title
      title typey-pad base

    block styles-core
      +link('/base.css')

    block styles

  body
    block layout
      h1 Example keypad
      +key('a')
      +key('b')
      +key('c')
      +key('Shift_L', 'Shift')
      +key('BackSpace', 'Back- space')

    block app-core
      +script('/socket.io/socket.io.js')
      +script('/lib-3p/jquery.js')
      +script('/keypad/app.js')

    block app
